  %IF{%SHOW_PRICE%,<td valign="top" width=%W_PRICE%%% %M_COLOR_TABLEHEAD_NOCOLORS% align="right"><b>%IF{%FINISHING_LIST_REBATE%,<JOPPS id=6013>折扣后价格</JOPPS>,<JOPPS id=6011>价格</JOPPS>}</b></td>,}
